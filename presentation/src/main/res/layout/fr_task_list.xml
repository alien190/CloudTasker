<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data class="TaskListBinding">

        <variable
            name="scopeName"
            type="String" />
    </data>

    <FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
